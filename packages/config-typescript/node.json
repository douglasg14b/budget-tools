{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node",
    "extends": "./base.json",
    "compilerOptions": {
        "rootDir": "${configDir}/src",
        "baseUrl": "${configDir}",
        "paths": {
            "@/*": ["${configDir}/src/*"]
        },
        "esModuleInterop": true
    }
}
